{
  "name": "hyperapp-logger",
  "version": "1.0.1",
  "description": "Log Hyperapp state updates and action information to the console",
  "main": "dist/logger.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HyperappCommunity/hyperapp-logger.git"
  },
  "files": [
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "test": "jest --coverage --no-cache",
    "build": "npm run bundle && npm run minify",
    "bundle": "rollup -i src/index.js -o dist/logger.js -m -f umd -n logger",
    "minify": "uglifyjs dist/logger.js -o dist/logger.js -mc pure_funcs=Object.defineProperty --source-map includeSources,url=logger.js.map",
    "prepare": "npm run build",
    "format": "prettier --semi false --write '{src,test}/**/*.js'"
  },
  "babel": {
    "presets": "env"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "hyperapp": "^1.1.2",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "rollup": "^0.52.2",
    "uglify-js": "^3.2.2"
  },
  "peerDependencies": {
    "hyperapp": "^1.1.2"
  },
  "author": "Wolfgang Wedemeyer <wolf@okwolf.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HyperappCommunity/hyperapp-logger/issues"
  },
  "homepage": "https://github.com/HyperappCommunity/hyperapp-logger"
}
